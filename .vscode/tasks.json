{
    "version": "2.0.0",
    "command": "sh",
    "type": "shell",
    "args": [
        "-c"
    ],
    "label": "all",
    "tasks": [
        {
            "label": "clean",
            "command": "make",
            "args": [
                "clean"
            ],
            "options": {
                "cwd": "${workspaceFolder}/SS/Projekat"
            }
        },
        {
            "label": "build",
            "command": "make",
            "dependsOn": [
                "cd",
                "clean"
            ],
            "dependsOrder": "sequence",
            "options": {
                "cwd": "${workspaceFolder}/SS/Projekat",
                "env": {
                    "DEBUG": 1
                }
            }
        }
    ]
}
